{
    "name": "yafou/visuel",
    "description": "",
    "keywords": [
        "faker",
        "data",
        "generator"
    ],
    "prefer-stable": true,
    "type": "library",
    "minimum-stability": "stable",
    "homepage": "https://github.com/YaFou/Visuel",
    "license": "MIT",
    "authors": [
        {
            "name": "Yannis Foucher",
            "homepage": "https://github.com/YaFou"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "YaFou\\Visuel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YaFou\\Visuel\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
